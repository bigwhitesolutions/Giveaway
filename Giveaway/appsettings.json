{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "File": {
      "Path": "app.log",
      "Append": true,
      "MinLevel": "Information",
      "LogLevel": {
        "Microsoft.Hosting": "Warning"
      },
      "FileSizeLimitBytes": 0,
      "MaxRollingFiles": 0
    }
  },
  "AllowedHosts": "*"
}
